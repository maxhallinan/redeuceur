{
  "name": "redeuceur",
  "version": "0.1.0",
  "description": "A utility for creating terse Redux reducers.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxhallinan/redeuceur"
  },
  "engines": {
    "node": ">=6"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf coverage dist",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "lint": "./node_modules/.bin/eslint src test",
    "prepublishOnly": "npm run clean && npm run lint && npm run build && npm run test",
    "test": "NODE_ENV=test npm run clean && npm run lint && npm run build && mocha test/**/*.js",
    "test:coverage": "NODE_ENV=test nyc npm run test",
    "test:watch": "mocha test/**/*.js --watch"
  },
  "author": {
    "name": "Max Hallinan",
    "email": "maxhallinan@riseup.net"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.1",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.3",
    "redux": "^3.7.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-watch": "^4.0.0"
  },
  "keywords": [
    "redux",
    "reducer"
  ],
  "files": [
    "dist"
  ]
}
